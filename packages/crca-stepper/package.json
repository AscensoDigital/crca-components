{
  "name": "@ascenso/crca-stepper",
  "version": "0.3.6",
  "description": "Webcomponent crca-stepper following open-wc recommendations",
  "author": "Claudio Corvalan",
  "license": "MIT",
  "main": "index.js",
  "module": "index.js",
  "scripts": {
    "start": "web-dev-server",
    "lint": "eslint --ext .js,.html . --ignore-path .gitignore && prettier \"**/*.js\" --check --ignore-path .gitignore",
    "format": "eslint --ext .js,.html . --fix --ignore-path .gitignore && prettier \"**/*.js\" --write --ignore-path .gitignore",
    "test": "web-test-runner --coverage",
    "test:watch": "web-test-runner --watch"
  },
  "dependencies": {
    "@ascenso/crca-status-light": "^0.3.3",
    "@material/mwc-button": "^0.25.3",
    "@material/mwc-icon-button": "^0.25.3",
    "lit": "^2.3.1"
  },
  "devDependencies": {
    "@open-wc/eslint-config": "^4.2.0",
    "@open-wc/testing": "^2.0.0",
    "@web/dev-server": "^0.1.1",
    "@web/test-runner": "^0.11.5",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^6.11.0",
    "husky": "^1.0.0",
    "lint-staged": "^10.0.0",
    "prettier": "^2.0.4"
  },
  "eslintConfig": {
    "extends": [
      "@open-wc/eslint-config",
      "eslint-config-prettier"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "arrowParens": "avoid"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "gitHead": "36e1bce0231ac95babeaf8b0655bd2b4d55b31b8"
}
